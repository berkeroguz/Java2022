package miniProjeAsalSayi;

public class Main {

	public static void main(String[] args) {
		
		int sayi=4;
		
		boolean asalMi=false;
		
		
		for (int i=1;i<sayi;i++)
		{
			if (sayi % i == 0)
			{
				System.out.println(i);
				break;
			}
			
		}
		asalMi=true;
		if(asalMi==true)
		{
			System.out.println("Sayı asaldır.");
		}
		else
		{
			System.out.println("sayı asal değildir.");
		}
		

	}

}
