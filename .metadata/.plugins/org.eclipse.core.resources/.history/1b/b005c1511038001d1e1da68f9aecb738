package arkadasSayilar;

public class Main {

	public static void main(String[] args) {

		int number1 = 240;
		int number2 = 24;
		int total1 = 0;
		int total2 = 0;

		for (int i = 1; i < number1; i++) {
			if (number1 % i == 0) {
				total1 = total1 + number1;
			}
		}
		for (int i = 1; i < number2; i++) {
			if (number2 % i == 0) {
				total2 = total2 + number2;
			}
		}
		if (total1==total2)
		{
			System.out.println("bu iki sayi arkadastir");
		}
		else
		{
			System.out.println("bu sayilar arkads degildir.");
		}

	}

}
